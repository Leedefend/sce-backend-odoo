# Compose project name
ENV=test
ENV_FILE=.env.test
COMPOSE_PROJECT_NAME=sc-backend-odoo-test

# Database
DB_USER=odoo
DB_PASSWORD=odoo
DB_NAME=sc_test

# Odoo master password
ADMIN_PASSWD=admin

# JWT secret (test placeholder)
JWT_SECRET=test-secret-change-me

# Odoo: lock to a single DB
ODOO_DBFILTER=^sc_test$

# Runtime data (named volumes, auto-prefixed by project)
DB_DATA=db_data
REDIS_DATA=redis_data
ODOO_DATA=odoo_data

# Host ports
NGINX_PORT=18082
ODOO_PORT=8071
