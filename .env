# Compose project name (pin)
COMPOSE_PROJECT_NAME=sc-backend-odoo

# Database
DB_USER=odoo
DB_PASSWORD=odoo
DB_NAME=sc_odoo

# Odoo master password (set as you like)
ADMIN_PASSWD=admin

# JWT secret (dev placeholder; production must be long random)
JWT_SECRET=dev-secret-change-me

# Odoo: only allow this DB (avoid db listing / cross-db accidents)
ODOO_DBFILTER=^sc_odoo$
