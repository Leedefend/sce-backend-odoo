<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record id="view_material_plan_to_rfq_wizard" model="ir.ui.view">
            <field name="name">material.plan.to.rfq.wizard.form</field>
            <field name="model">material.plan.to.rfq.wizard</field>
            <field name="arch" type="xml">
                <form string="生成询价单">
                    <group>
                        <field name="partner_id"/>
                        <field name="note"/>
                    </group>
                    <footer>
                        <button string="取消" class="btn-secondary" special="cancel"/>
                        <button name="action_generate_rfq" string="生成询价单" type="object" class="btn-primary"/>
                    </footer>
                </form>
            </field>
        </record>

        <record id="action_material_plan_to_rfq_wizard" model="ir.actions.act_window">
            <field name="name">生成询价单</field>
            <field name="res_model">material.plan.to.rfq.wizard</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
        </record>
    </data>
</odoo>
