<odoo>
    <record id="project_stage_planning" model="project.project.stage">
        <field name="name">筹备中</field>
        <field name="sequence">5</field>
        <field name="is_default">True</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="project_stage_running" model="project.project.stage">
        <field name="name">执行中</field>
        <field name="sequence">10</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="project_stage_closed" model="project.project.stage">
        <field name="name">已竣工</field>
        <field name="sequence">20</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
