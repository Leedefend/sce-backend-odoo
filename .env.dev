# Compose project name
ENV=dev
ENV_FILE=.env.dev
COMPOSE_PROJECT_NAME=sc-backend-odoo-dev

# Database
DB_USER=odoo
DB_PASSWORD=odoo
DB_NAME=sc_demo

# Odoo master password
ADMIN_PASSWD=admin

# JWT secret (dev placeholder)
JWT_SECRET=dev-secret-change-me

# Odoo: lock to a single DB
ODOO_DBFILTER=^sc_demo$

# Runtime data (named volumes, auto-prefixed by project)
DB_DATA=db_data
REDIS_DATA=redis_data
ODOO_DATA=odoo_data

# Host ports
NGINX_PORT=18081
ODOO_PORT=8070
